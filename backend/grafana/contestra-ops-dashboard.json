{
  "dashboard": {
    "__inputs": [
      {
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "description": "",
        "type": "datasource",
        "pluginId": "prometheus",
        "pluginName": "Prometheus"
      }
    ],
    "__requires": [
      {
        "type": "grafana",
        "id": "grafana",
        "name": "Grafana",
        "version": "10.0.0"
      },
      {
        "type": "panel",
        "id": "stat",
        "name": "Stat",
        "version": ""
      },
      {
        "type": "panel",
        "id": "timeseries",
        "name": "Time series",
        "version": ""
      },
      {
        "type": "panel",
        "id": "gauge",
        "name": "Gauge",
        "version": ""
      },
      {
        "type": "panel",
        "id": "table",
        "name": "Table",
        "version": ""
      },
      {
        "type": "datasource",
        "id": "prometheus",
        "name": "Prometheus",
        "version": "2.0.0"
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": false,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "name": "Auth Expiring (<4h)",
          "datasource": "${DS_PROMETHEUS}",
          "enable": true,
          "expr": "ALERTS{alertname=\"AuthTokenExpiringSoon\",alertstate=\"firing\"}",
          "iconColor": "rgba(255, 140, 0, 1)",
          "step": "30s",
          "tagKeys": "severity,alertname",
          "titleFormat": "{{alertname}}",
          "type": "dashboard"
        },
        {
          "name": "Rotating Proxy Detected",
          "datasource": "${DS_PROMETHEUS}",
          "enable": true,
          "expr": "ALERTS{alertname=\"ProxyModeRotatingDetected\",alertstate=\"firing\"}",
          "iconColor": "rgba(255, 96, 96, 1)",
          "step": "30s",
          "tagKeys": "severity,alertname",
          "titleFormat": "{{alertname}}",
          "type": "dashboard"
        },
        {
          "name": "OpenAI 429 > 3%",
          "datasource": "${DS_PROMETHEUS}",
          "enable": true,
          "expr": "ALERTS{alertname=\"OpenAIHighRateLimitRate\",alertstate=\"firing\"}",
          "iconColor": "rgba(255, 96, 96, 1)",
          "step": "30s",
          "tagKeys": "severity,alertname",
          "titleFormat": "{{alertname}}",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "id": null,
    "iteration": 1,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "type": "stat",
        "title": "Auth Token — Seconds Remaining",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "max by (auth_mode) (contestra_auth_token_seconds_remaining)",
            "legendFormat": "{{auth_mode}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          },
          "orientation": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 14400
                },
                {
                  "color": "green",
                  "value": 86400
                }
              ]
            }
          },
          "overrides": []
        }
      },
      {
        "type": "stat",
        "title": "Auth Status",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "contestra_auth_status{status=\"ok\"}",
            "legendFormat": "OK",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          },
          "orientation": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "1": {
                    "text": "Healthy",
                    "color": "green"
                  }
                }
              },
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "Unhealthy",
                    "color": "red"
                  }
                }
              }
            ]
          },
          "overrides": []
        }
      },
      {
        "type": "gauge",
        "title": "Proxy Mode",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "contestra_proxy_mode",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "-1": {
                    "text": "unknown"
                  }
                }
              },
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "direct"
                  }
                }
              },
              {
                "type": "value",
                "options": {
                  "1": {
                    "text": "backbone"
                  }
                }
              },
              {
                "type": "value",
                "options": {
                  "2": {
                    "text": "rotating"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "blue",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            },
            "min": -1,
            "max": 2
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          }
        }
      },
      {
        "type": "table",
        "title": "Proxy RTT",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "contestra_proxy_rtt_ms{service=~\".+\",probe=~\"first|second|secondary\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": []
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Time"
              },
              "properties": [
                {
                  "id": "custom.hidden",
                  "value": true
                }
              ]
            }
          ]
        }
      },
      {
        "type": "timeseries",
        "title": "LLM p95 Latency by Vendor/Path/Proxied",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 24,
          "h": 10
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (le, vendor, path, proxied) (rate(contestra_llm_latency_ms_bucket{vendor=~\"$vendor\", path=~\"$path\", proxied=~\"$proxied\"}[$__rate_interval])))",
            "legendFormat": "{{vendor}}/{{path}} proxied={{proxied}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10,
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "thresholdsStyle": {
                "mode": "dashed"
              }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 60000
                },
                {
                  "color": "red",
                  "value": 120000
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "lastNotNull",
              "mean",
              "max"
            ]
          }
        }
      },
      {
        "type": "timeseries",
        "title": "OpenAI 429 Rate (%) - 10m",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 0,
          "y": 18,
          "w": 12,
          "h": 9
        },
        "targets": [
          {
            "expr": "100 * ( sum(increase(contestra_llm_rate_limit_events_total{vendor=\"openai\"}[10m])) / clamp_min(sum(increase(contestra_llm_latency_ms_count{vendor=\"openai\"}[10m])), 1) )",
            "legendFormat": "429%",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "thresholdsStyle": {
                "mode": "dashed+area"
              }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "type": "timeseries",
        "title": "Throughput (Requests/min) by Vendor/Path",
        "datasource": "${DS_PROMETHEUS}",
        "gridPos": {
          "x": 12,
          "y": 18,
          "w": 12,
          "h": 9
        },
        "targets": [
          {
            "expr": "sum by (vendor, path) (increase(contestra_llm_latency_ms_count[$__interval]))",
            "legendFormat": "{{vendor}}/{{path}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 80,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "normal",
                "group": "A"
              }
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": [
              "sum",
              "mean"
            ]
          }
        }
      }
    ],
    "refresh": "15s",
    "schemaVersion": 38,
    "style": "dark",
    "tags": [
      "contestra",
      "ops",
      "llm"
    ],
    "templating": {
      "list": [
        {
          "type": "query",
          "name": "vendor",
          "label": "Vendor",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(contestra_llm_latency_ms_bucket, vendor)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "type": "query",
          "name": "path",
          "label": "Path",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(contestra_llm_latency_ms_bucket, path)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "type": "query",
          "name": "proxied",
          "label": "Proxied",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(contestra_llm_latency_ms_bucket, proxied)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "15s",
        "30s",
        "1m",
        "5m"
      ]
    },
    "timezone": "",
    "title": "Contestra — LLM Ops (Auth, Proxy, Latency, 429, Throughput)",
    "version": 1,
    "weekStart": ""
  },
  "overwrite": false
}